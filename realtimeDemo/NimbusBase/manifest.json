{
  "name": "NimbusBase",
  "description": "Test NimbusBase",
  "version": "1.0",
  "manifest_version": 2,  
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*", 
    "https://*/*",
    "unlimitedStorage"
  ],
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'"
}
